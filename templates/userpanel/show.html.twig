{% extends 'frontbase.html.twig' %}


{% block title %}Üye Profil Bilgileri{% endblock %}


{% block sidebar %} {{ include('home/_sidebar.html.twig') }} {% endblock %}



{% block body %}

    <div class="row-fluid">
        <div class="span9">

            <div class="span4">

                {% block usersidebar %} {{ include('home/_usersidebar.html.twig') }} {% endblock %}
            </div>

            <div class="span5">
                <h5>Üye Profili</h5>
                {{ include('messages.html.twig') }}

                <table class="table">
                    <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ app.user.id }}</td>
                    </tr>
                    <tr>
                        <th>Userid</th>
                        <td>{{ app.user.email }}</td>
                    </tr>
                    <tr>
                        <th>Roles</th>
                        <td>{{ app.user.roles|join }}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{ app.user.name }}</td>
                    </tr>
                    <tr>
                        <th>Address</th>
                        <td>{{ app.user.address }}</td>
                    </tr>
                    <tr>
                        <th>City</th>
                        <td>{{ app.user.city }}</td>
                    </tr>
                    <tr>
                        <th>Phone</th>
                        <td>{{ app.user.phone }}</td>
                    </tr>

                    <tr>
                        <th>Status</th>
                        <td>{{ app.user.status }}</td>
                    </tr>

                    </tbody>
                </table>


            </div>

            <div class="row-fluid"></div>
        </div>
    </div>



{% endblock %}


